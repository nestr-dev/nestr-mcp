{
  "name": "@nestr/mcp",
  "version": "0.1.9",
  "description": "MCP server for Nestr - connect AI assistants to your workspace",
  "author": "Nestr <dev@nestr.io>",
  "license": "MIT",
  "type": "module",
  "main": "build/index.js",
  "bin": {
    "nestr-mcp": "./build/index.js"
  },
  "files": [
    "build",
    "web"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "start:http": "node build/http.js",
    "dev": "tsx src/index.ts",
    "dev:http": "tsx src/http.ts",
    "inspect": "npx @modelcontextprotocol/inspector node build/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "nestr",
    "ai",
    "claude",
    "anthropic",
    "cursor",
    "holacracy",
    "self-organization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nestr-dev/nestr-mcp"
  },
  "homepage": "https://mcp.nestr.io",
  "bugs": {
    "url": "https://github.com/nestr-dev/nestr-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "express": "^5.0.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
